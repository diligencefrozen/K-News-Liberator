{
    "name": "NNGN-NaverNewstoGoogleNews",
    "description": "Force Naver News Page to Google News Page. It is designed for elderly South Koreans who only use Naver News.",
    "version": "23.0719.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "permissions": [

    ],
    "host_permissions": [
      "*://*.naver.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.naver.com/*"],
        "js": ["index.js"]
      }
    ]
  }
  
